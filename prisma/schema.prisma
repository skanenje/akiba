generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Project {
  id          Int      @id @default(autoincrement())
  name        String
  description String?
  category    String
  amount      Float    @default(0)
  donations   Int      @default(0)
  active      Boolean  @default(true)
}